    for i = 1, resultsCount do
      gg.addListItems({[1] = {address = results[i].address + 48,flags = 4,freeze = true,value = 70032}})
      gg.addListItems({[1] = {address = results[i].address + 100,flags = 4,freeze = true,value = 4452}})
    end
