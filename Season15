function CLOSE(...)
  print("â–Œâ”‚â–ˆâ•‘â–Œâ•‘â–Œâ•‘ğŸ‡²ğŸ‡² ğŠğ’ğ‹ ğ†ğ€ğŒğˆğğ† ğŸ‡²ğŸ‡² â•‘â–Œâ•‘â–Œâ•‘â–ˆâ”‚â–Œ")
  gg.skipRestoreState()
  gg.setVisible(true)
  os.exit()
end

START() 
while true do
  if gg.isVisible(true) then 
     PUBGMH = 1
    gg.setVisible(false)
  end
  if PUBGMH == 1 then
    START()
  end
 end
